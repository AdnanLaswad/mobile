<?xml version="1.0" encoding="utf-8" ?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    


    <color name="message_error">#FF4444</color>
    <color name="message_completed">#99CC00</color>
    <color name="message_success">#333333</color>
    <color name="theme_background_color">#FFF</color>
	<color name="alt_theme_color">#0099CC</color>    
	<color name="theme_color">#C50022</color>
	
    <color name="item_touched_color">#77DDFF</color>
    <color name="alt_item_touched_color">#33B5E5</color>
    
    <color name="list_item_selected_color">#33B5E5</color>
    <color name="list_item_color">#FFFFFF</color>
    <color name="details_color">#FFFFFF</color>
    <color name="data_color">#EFEFEF</color>
    <color name="label_color">#A3A3A3</color>
    <color name="container_frame_color">#EFEFEF</color>
    <color name="data_frame_color">#FFFFFF</color>
    <color name="detail_frame_color">#EFEFEF</color>
    <color name="separator_color">#EFEFEF</color>
    <color name="container_color">#FFFFFF</color>
    <color name="button_cancel_color">#0099CC</color>
    <color name="button_cancel_touched_color">#33B5E5</color>

    <style name="AppTheme" parent="android:Theme.Holo.Light">
        <item name="android:actionBarStyle">@style/ActionBar</item>
        <item name="android:dropDownListViewStyle">
            @android:style/Widget.Holo.ListView.DropDown
        </item>
        <item name="android:actionOverflowButtonStyle">@style/ActionOverflowStyle</item>
        <item name="android:homeAsUpIndicator">@drawable/ic_action_back</item>
    </style>

    <style name="AppThemeNoBar" parent="AppTheme">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="ActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/action_bar</item>
        <item name="android:icon">@android:color/transparent</item>
        <item name="android:actionOverflowButtonStyle">@style/ActionOverflowStyle</item>
        <item name="android:homeAsUpIndicator">@drawable/ic_action_back</item>
    </style>
    
    <style name="ActionOverflowStyle" parent="@android:style/Widget.Holo.Light.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_action_overflow</item>
    </style>
	
    
    
    <style name="FormLabel" parent="android:TextAppearance.Small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal|left</item>
    </style>

    <style name="Form">
        <item name="android:orientation">vertical</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingTop">10dp</item>
    </style>

    <style name="FormEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:background">@drawable/text_edit</item>
    </style>
    
    <style name="FormEditTextPW">
        <item name="android:layout_width">match_parent</item>
        <item name="android:typeface">sans</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">15dp</item>
        <item name="android:background">@drawable/text_edit_pw</item>
    </style>
    
    <style name="FormEditTextUN">
        <item name="android:layout_width">match_parent</item>
        <item name="android:typeface">sans</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:background">@drawable/text_edit_un</item>
    </style>
    
    <style name="StatusTitle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:typeface">sans</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@color/detail_frame_color</item>
    </style>
    
    <style name="Button" parent="android:TextAppearance.Small">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/button</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    
    <style name="LeftButton" parent="android:TextAppearance.Small">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:background">@drawable/button_cancel</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="RightButton" parent="android:TextAppearance.Small">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:background">@drawable/button</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="ButtonLogin" parent="android:TextAppearance.Small">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/button_login</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="ButtonRow">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@color/details_color</item>
    </style>

    <style name="ButtonCancel" parent="Button">
        <item name="android:background">@drawable/button_cancel</item>
    </style>

    <style name="Container">
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:background">@color/container_color</item>
    </style>

    <style name="ContainerWithFrame" parent="Container">
        <item name="android:background">@color/container_frame_color</item>
    </style>

    <style name="ListView">
        <item name="android:divider">@color/container_frame_color</item>
        <item name="android:dividerHeight">8dp</item>
    </style>

    <style name="ListViewPlain">
        <item name="android:dividerHeight">0dp</item>
        <item name="android:divider">@null</item>
    </style>


    <style name="Details">
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:background">@color/details_color</item>
    </style>
    <style name="Data">
        <item name="android:paddingLeft">205dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:background">@color/data_color</item>
    </style>
    <style name="Label" parent="android:TextAppearance.Small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/label_color</item>
    </style>

    <style name="DetailsValue" parent="android:TextAppearance.Small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
    </style>

    
    <style name="Heading" parent="android:TextAppearance.Medium">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
    </style>
        
    <style name="StatusValue" parent="android:TextAppearance.Medium">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
        <item name="android:textColor">@color/label_color</item>
    </style>

    <style name="Disclaimer">
        <item name="android:textSize">26sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingMultiplier">1.5</item>
        <item name="android:padding">10dp</item>
        <item name="android:gravity">center</item>
    </style>
        <style name="Support">
        <item name="android:textSize">26sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingMultiplier">1.5</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_marginBottom">25dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:gravity">center</item>
    </style>
        <style name="SupportInfo">
        <item name="android:textSize">18sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingMultiplier">1.5</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">10dp</item>
        
    </style>
</resources>
